package com.shop.dto;

import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;
import lombok.ToString;

@Getter
@Setter

// 기본 생성자를 자동으로 만들어준다
@NoArgsConstructor
@ToString
public class MemberDto {

	private String id;
	private String password;
	private String name;
	private String email;
	
}
